{
  "name": "jordan-papaleo",
  "description": "Personal website",
  "version": "1.0.0",
  "private": true,
  "author": "Jordan Papaleo <jordanpapaleo@gmail.com>",
  "keywords": [],
  "license": "none",
  "main": "n/a",
  "scripts": {
    "build:gatsby": "NODE_ENV=production gatsby build",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "build": "npm run build:gatsby; npm run build:storybook",
    "clean": "rimraf public .cache",
    "dev": "NODE_ENV=development gatsby develop",
    "fix:format": "$(npm bin)/prettier --write 'src/**/*.js'",
    "fix:lint": "$(npm bin)/eslint 'src/**/*.js' --cache --fix",
    "fix": "npm run fix:format && npm run fix:lint",
    "purge": "rm -rf node_modules package-lock.json; npm i",
    "serve": "gatsby serve",
    "start": "npm run dev",
    "start:sb": "start-storybook -p 8001 -c .storybook",
    "test:fe": "npm run test:fe:lint && npm run test:fe:types && npm run test:fe:unit",
    "test:fe:lint": "$(npm bin)/eslint './src/**'",
    "test:fe:unit": "$(npm bin)/jest -c ./jest.config.js",
    "test:fe:dev": "$(npm bin)/jest -c ./jest.config.js --watch",
    "test:fe:types": "$(npm bin)/flow",
    "test:fe:e2e:dev": "$(npm bin)/cypress open",
    "test:fe:e2e": "$(npm bin)/cypress run ",
    "test": "npm run test:fe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanpapaleo/gatsby-flow-storybook-test"
  },
  "bugs": {
    "url": "https://github.com/jordanpapaleo/gatsby-flow-storybook-test/issues"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "classnames": "2.2.6",
    "lodash": "4.17.15",
    "modern-normalize": "0.5.0",
    "normalize.css": "8.0.1",
    "polished": "3.4.0",
    "prism-themes": "1.1.0",
    "prismjs": "1.16.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-icons-kit": "1.3.1",
    "react-typography": "0.16.19",
    "styled-jsx": "3.2.4",
    "typography": "0.16.19",
    "typography-theme-stern-grove": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-flow": "7.0.0",
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-console": "1.2.1",
    "@storybook/addon-knobs": "5.3.12",
    "@storybook/addon-links": "5.3.12",
    "@storybook/addon-notes": "5.3.12",
    "@storybook/addons": "5.3.12",
    "@storybook/react": "5.3.12",
    "@types/classnames": "2.2.8",
    "@types/node": "12.0.4",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "apollo-link-http": "1.5.15",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-react-require": "3.1.3",
    "babel-preset-gatsby": "0.2.29",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-react-app": "3.0.8",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "flow-bin": "0.100.0",
    "gatsby": "2.13.39",
    "gatsby-image": "2.2.6",
    "gatsby-plugin-emotion": "4.1.2",
    "gatsby-plugin-feed": "2.3.5",
    "gatsby-plugin-flow": "1.1.2",
    "gatsby-plugin-google-analytics": "2.1.4",
    "gatsby-plugin-manifest": "2.2.4",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-react-helmet": "3.1.2",
    "gatsby-plugin-resolve-src": "2.0.0",
    "gatsby-plugin-sharp": "2.2.9",
    "gatsby-plugin-stripe": "1.2.3",
    "gatsby-plugin-styled-jsx": "3.1.18",
    "gatsby-plugin-typography": "2.3.2",
    "gatsby-remark-copy-linked-files": "2.1.3",
    "gatsby-remark-images": "3.1.7",
    "gatsby-remark-prismjs": "3.3.3",
    "gatsby-remark-responsive-iframe": "2.2.4",
    "gatsby-remark-smartypants": "2.1.2",
    "gatsby-source-filesystem": "2.1.6",
    "gatsby-source-graphql": "2.1.3",
    "gatsby-transformer-remark": "2.6.9",
    "gatsby-transformer-sharp": "2.2.4",
    "graphql": "14.4.2",
    "husky": "4.2.3",
    "jest": "24.9.0",
    "lint-staged": "10.0.7",
    "prettier": "1.18.2",
    "raw-loader": "2.0.0",
    "react-markdown": "4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run fix"
  }
}
